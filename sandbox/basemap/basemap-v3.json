{
  "version": "0.0.1",
  "name": "opm-mars-basemap-v3",
  "auth": {
    "method": "open"
  },
  "layergroup": {
    "version": "1.0.1",
    "layers": [

      {
        "type": "plain",
        "options": {
            "color": "#965e43",
            "imageUrl": null
        }
      },

      {
        "type": "cartodb",
        "options": {
          "cartocss_version": "2.1.1",
          "cartocss": "#layer { polygon-fill: #374c70; polygon-opacity: 0.0; line-width: 1; line-color: #FFF; line-opacity: 0.5; }",
          "sql": "SELECT * FROM nmanaud.mars_contours_200m"
        }
      }

    ]
  }
}
